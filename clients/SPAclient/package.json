{
  "name": "modelhostess",
  "author": "Michael Nakonechny",
  "version": "0.0.1",
  "description": "ModelHostess",
  "repository": "https://github.com/PhoboSys/modelhostess",
  "private": true,
  "main": "./bin/app.js",
  "dependencies": {
    "classnames": "2.2.1",
    "crossroads": "0.12.2",
    "flux": "2.1.1",
    "lodash": "3.8.0",
    "react": "0.13.1",
    "qwest": "2.2.4"
  },
  "devDependencies": {
    "bourbon": "~4.2.6",
    "gulp": "~3.9.0",
    "gulp-chmod": "~1.3.0",
    "gulp-concat": "~2.6.0",
    "gulp-eslint": "~1.1.1",
    "gulp-sass": "~2.1.0",
    "gulp-streamify": "~1.0.2",
    "gulp-uglify": "~1.5.1",
    "gulp-util": "~3.0.7",
    "gulp-html-replace": "~1.5.5",
    "harmonize": "~1.4.4",
    "browserify": "~12.0.1",
    "eslint": "~1.10.3",
    "eslint-plugin-react": "~3.11.3",
    "reactify": "~1.1.1",
    "envify": "~3.4.0",
    "uglify-js": "~2.6.1",
    "watchify": "~3.6.1",
    "through2": "~2.0.0",
    "vinyl-source-stream": "~1.1.0",
    "vinyl-buffer": "~1.0.0",
    "del": "~2.1.0",
    "merge-stream": "1.0.0"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
      "jsx": true
    },
    "rules": {
      "react/jsx-uses-react": 1,
      "quotes": [
        0
      ],
      "strict": [
        2,
        "global"
      ],
      "indent": [
        2,
        4
      ],
      "consistent-return": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "new-cap": [
        0
      ],
      "curly": [
        2,
        "multi-line"
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "no-empty": [
        1
      ],
      "no-shadow": [
        1
      ],
      "no-undef": [
        2
      ]
    },
    "env": {
      "browser": true,
      "amd": true,
      "node": true
    }
  }
}
