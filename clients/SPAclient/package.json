{
  "name": "modelhostess",
  "author": "Michael Nakonechny",
  "version": "0.0.1",
  "description": "ModelHostess",
  "repository": "https://github.com/PhoboSys/modelhostess",
  "private": true,
  "main": "./bin/app.js",
  "dependencies": {
    "classnames": "2.2.1",
    "crossroads": "0.12.2",
    "flux": "2.1.1",
    "lodash": "3.8.0",
    "react": "0.13.1"
  },
  "devDependencies": {
    "bourbon": "4.2.3",
    "gulp": "3.8.11",
    "gulp-concat": "2.5.2",
    "gulp-eslint": "0.12.0",
    "gulp-jest": "0.4.0",
    "gulp-sass": "2.0.4",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "1.2.0",
    "gulp-util": "3.0.4",
    "harmonize": "1.4.2",
    "browserify": "6.2.0",
    "envify": "3.0.0",
    "eslint": "0.21.0",
    "eslint-plugin-react": "2.2.0",
    "jest": "0.1.39",
    "jest-cli": "0.4.1",
    "reactify": "0.15.2",
    "react-tools": "0.13.3",
    "uglify-js": "2.4.15",
    "watchify": "2.1.1"
  },
  "jest": {
    "scriptPreprocessor": "./bin/preprocessor.js",
    "unmockedModulePathPatterns": [
      "react"
    ],
    "collectCoverage": true
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
      "jsx": true
    },
    "rules": {
      "react/jsx-uses-react": 1,
      "quotes": [
        0
      ],
      "global-strict": [
        2,
        "always"
      ],
      "indent": [
        2,
        "tab"
      ],
      "consistent-return": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "new-cap": [
        0
      ],
      "curly": [
        2,
        "multi-line"
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "no-empty": [
        1
      ],
      "no-shadow": [
        1
      ]
    },
    "env": {
      "browser": true,
      "amd": true,
      "node": true
    }
  }
}
