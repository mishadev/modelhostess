{
  "name": "modelhostess",
  "version": "0.0.1",
  "description": "ModelHostess",
  "repository": "https://github.com/DreamHostData/Remixer",
  "main": "js/app.js",
  "dependencies": {
    "classnames": "2.1.3",
    "color": "0.10.1",
    "crossroads": "0.12.2",
    "crypto-js": "3.1.4",
    "del": "1.1.1",
    "dropzone": "4.0.1",
    "es6-promise": "2.0.1",
    "flux": "2.0.1",
    "foundation-sites": "5.5.2",
    "handlebars": "3.0.3",
    "hasher": "1.2.0",
    "jquery": "2.1.3",
    "lodash": "3.8.0",
    "lodash.assign": "3.1.0",
    "mdi": "1.2.65",
    "merge-stream": "0.1.7",
    "object-assign": "1.0.0",
    "react": "0.13.1",
    "react-ab": "0.4.3",
    "react-draggable": "0.4.0",
    "react-dropzone": "2.2.4",
    "react-infinite-scroll": "0.1.0",
    "react-masonry-mixin": "0.5.7",
    "react-remarkable": "1.1.1",
    "react-tagsinput": "1.4.7",
    "react-tap-event-plugin": "0.1.6",
    "react-timer-mixin": "0.13.2",
    "react-tools": "0.13.1",
    "remarkable": "1.6.0",
    "sockjs-client": "1.0.3",
    "through2": "0.6.5",
    "velocity-animate": "1.2.2",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "vinyl-transform": "1.0.0"
  },
  "devDependencies": {
    "bourbon": "4.2.3",
    "gulp": "3.8.11",
    "gulp-concat": "2.5.2",
    "gulp-eslint": "0.12.0",
    "gulp-if": "1.2.5",
    "gulp-jest": "0.4.0",
    "gulp-sass": "2.0.4",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "1.2.0",
    "gulp-util": "3.0.4",
    "harmonize": "1.4.2",
    "browserify": "6.2.0",
    "catw": "0.2.0",
    "envify": "3.0.0",
    "eslint": "0.21.0",
    "eslint-plugin-react": "2.2.0",
    "jest": "0.1.39",
    "jest-cli": "0.4.1",
    "reactify": "0.15.2",
    "uglify-js": "2.4.15",
    "watchify": "2.1.1"
  },
  "author": "DreamHost",
  "jest": {
    "rootDir": "./js",
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "react"
    ],
    "collectCoverage": true
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
      "jsx": true
    },
    "rules": {
      "react/jsx-uses-react": 1,
      "quotes": [
        0
      ],
      "global-strict": [
        2,
        "always"
      ],
      "indent": [
        2,
        "tab"
      ],
      "consistent-return": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "new-cap": [
        0
      ],
      "curly": [
        2,
        "multi-line"
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "no-empty": [
        1
      ],
      "no-shadow": [
        1
      ]
    },
    "env": {
      "browser": true,
      "amd": true,
      "node": true
    }
  }
}
